{
    "name": "techtrans/laravel-oauth2-client",
    "description": "Laravel OAuth2 Client",
    "keywords": [
        "techtrans",
        "laravel-oauth2-client"
    ],
    "homepage": "https://github.com/atouiahmed/laravel-oauth2-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Colin Hall",
            "email": "colin@macsi.co.uk"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "atouiahmed\\OAuth2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "atouiahmed\\OAuth2\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "atouiahmed\\OAuth2\\Providers\\OAuth2ServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
